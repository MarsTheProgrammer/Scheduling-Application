<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="324.0" prefWidth="595.0">
         <children>
            <VBox layoutX="204.0" layoutY="106.0" prefHeight="113.0" prefWidth="188.0">
               <children>
                  <TextField fx:id="usernameTxtField" prefHeight="25.0" prefWidth="157.0" promptText="Enter username">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id ="passwordTxtField" promptText="Enter password">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </TextField>
               </children></VBox>
            <Label layoutX="136.0" layoutY="106.0" prefHeight="25.0" prefWidth="56.0" text="Username:" />
            <Label layoutX="136.0" layoutY="162.0" prefHeight="25.0" prefWidth="62.0" text="Password:" />
            <Label layoutX="485.0" layoutY="12.0" text="Zone:" />
            <Label fx:id="zoneID" layoutX="522.0" layoutY="12.0" text="en_US" />
            <Button fx:id="loginButton" onAction="#onActionLogin" layoutX="496.0" layoutY="277.0" mnemonicParsing="false" text="Login" />
         </children>
      </Pane>
   </children>
</GridPane>
